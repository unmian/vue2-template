{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "api": [
        "./src/api/index"
      ],
      "api/*": [
        "./src/api/*"
      ],
      "components": [
        "./src/components/index"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "config": [
        "./src/config/index"
      ],
      "config/*": [
        "./src/config/*"
      ],
      "constants": [
        "./src/constants/index"
      ],
      "constants/*": [
        "./src/constants/*"
      ],
      "data": [
        "src/data/index"
      ],
      "data/*": [
        "src/data/*"
      ],
      "directives": [
        "./src/directives/index"
      ],
      "directives/*": [
        "./src/directives/*"
      ],
      "filters": [
        "./src/filters/indx"
      ],
      "filters/*": [
        "./src/filters/*"
      ],
      "lib": [
        "./src/lib/index"
      ],
      "lib/*": [
        "./src/lib/*"
      ],
      "router": [
        "./src/router/index"
      ],
      "router/*": [
        "./src/router/*"
      ],
      "store": [
        "./src/store/index"
      ],
      "store/*": [
        "./src/store/*"
      ],
      "types": [
        "./src/types/index"
      ],
      "types/*": [
        "./src/types/*"
      ],
      "utils": [
        "./src/utils/index"
      ],
      "utils/*": [
        "./src/utils/*"
      ],
      "views": [
        "./src/views/index"
      ],
      "views/*": [
        "./src/views/*"
      ]
    },
    "types": [
      "vite/client"
    ]
  },
  "vueCompilerOptions": {
    "experimentalCompatMode": 2
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue"
  ],
  "exclude": [
    "node_modules"
  ]
}